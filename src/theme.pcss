
:root {
    /* =~= Theme Properties =~= */
    --theme-font-family-base: system-ui;
    --theme-font-family-heading: system-ui;
    --theme-font-color-base: 0 0 0;
    --theme-font-color-dark: 255 255 255;
    --theme-rounded-base: 9999px;
    --theme-rounded-container: 8px;
    --theme-border-base: 1px;
    /* =~= Theme On-X Colors =~= */
    --on-primary: 0 0 0;
    --on-secondary: 255 255 255;
    --on-tertiary: 0 0 0;
    --on-success: 0 0 0;
    --on-warning: 0 0 0;
    --on-error: 0 0 0;
    --on-surface: 255 255 255;
    /* =~= Theme Colors  =~= */
    /* primary | #0fb877 */
    --color-primary-50: 219 244 235; /* ⬅ #dbf4eb */
    --color-primary-100: 207 241 228; /* ⬅ #cff1e4 */
    --color-primary-200: 195 237 221; /* ⬅ #c3eddd */
    --color-primary-300: 159 227 201; /* ⬅ #9fe3c9 */
    --color-primary-400: 87 205 160; /* ⬅ #57cda0 */
    --color-primary-500: 15 184 119; /* ⬅ #0fb877 */
    --color-primary-600: 14 166 107; /* ⬅ #0ea66b */
    --color-primary-700: 11 138 89; /* ⬅ #0b8a59 */
    --color-primary-800: 9 110 71; /* ⬅ #096e47 */
    --color-primary-900: 7 90 58; /* ⬅ #075a3a */
    /* secondary | #694CF2 */
